<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mRelativeLayout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_50"
    android:layout_margin="@dimen/dp_7"
    android:background="@drawable/shape_voter_item_background">

    <com.xw.aschwitkey.view.CircleImageView
        android:id="@+id/mImageView_head"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        android:layout_margin="@dimen/dp_10"
        android:src="@mipmap/default_icon"/>

    <TextView
        android:id="@+id/mTextView_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@+id/mImageView_head"
        android:text="123"
        android:textColor="@color/main2"
        android:textSize="@dimen/sp_12" />


    <TextView
        android:id="@+id/mTextView_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="@dimen/dp_5"
        android:layout_marginBottom="5dp"
        android:textColor="@color/TextColor1"
        android:textSize="@dimen/sp_9" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mTextView_nickname"
        android:layout_toRightOf="@id/mImageView_head">

        <TextView
            android:id="@+id/mTextView_currency"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="â‰ˆ"
            android:textColor="@color/text_bg"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/mTextView_m"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_5"
            android:text="0"
            android:textColor="@color/text_bg"
            android:textSize="@dimen/sp_10" />
    </LinearLayout>

</RelativeLayout>
